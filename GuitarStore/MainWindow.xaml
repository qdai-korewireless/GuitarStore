<Window x:Class="GuitarStore.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525" Loaded="Window_Loaded">
    <Grid >
        <DataGrid HorizontalAlignment="Left" Margin="76,71,0,0" VerticalAlignment="Top" Height="218" Width="432" Name="dataGridInventory"/>
        <ComboBox HorizontalAlignment="Left" Margin="76,13,0,0" VerticalAlignment="Top" Width="120" Name="comboBoxGuitarTypes" SelectionChanged="comboBoxGuitarTypes_SelectionChanged"/>
        <Label Name="labelMessage" Content="Messages" HorizontalAlignment="Left" Margin="76,40,0,0" VerticalAlignment="Top" Width="143"/>
        <Button Name="buttonViewSQL" Content="View SQL" HorizontalAlignment="Left" Margin="412,26,0,0" VerticalAlignment="Top" Width="75" Click="buttonViewSQL_Click"/>
        <Label Name="labelTraffic" Content="Traffic" HorizontalAlignment="Left" Margin="340,26,0,0" VerticalAlignment="Top" Width="64"/>
        <Button Content="Delete" Name="buttonDelete" HorizontalAlignment="Left" Margin="263,13,0,0" VerticalAlignment="Top" Width="75" Click="buttonDelete_Click"/>

    </Grid>
</Window>
